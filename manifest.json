{
  "manifest_version":3 ,
  "name":"Lock In Blocker",
  "version":"0.0.1",
  "description":"You get berated by Blue Lock characters if you stray away to blocked websites",
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["index.js"]
    }
  ],
  "icons":{
    "16":"icon16.png",
    "48":"icon48.png",
    "128":"icon128.png"
  },
  "web_accessible_resources": [{
    "resources": ["blocker.html", "blocker.js", "BlueLockAudio/*.mp3"],
    "matches": ["<all_urls>"]
  }]
}